{
  "name": "zesty-sdk-utils",
  "version": "1.0.0",
  "description": "Scripts with functionality shared between SDKs. Should not be published.",
  "main": "networking.js",
  "scripts": {
    "build": "tsc && esbuild dist/formats.js --format=iife --global-name=zestyFormats --bundle --minify --outfile=dist/zesty-formats.js",
    "build-networking": "tsc && esbuild dist/networking.js --format=esm --global-name=zestyNetworking --bundle --minify --outfile=dist/zesty-networking.js",
    "build-ts": "tsc",
    "build-legacy": "esbuild formats.js --format=iife --global-name=zestyFormats --bundle --minify --outfile=dist/zesty-formats.js && esbuild networking.js --format=esm --global-name=zestyNetworking --bundle --minify --outfile=dist/zesty-networking.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "sig-beacon": "^0.0.14",
    "three": "^0.166.1",
    "uuid": "^8.3.2"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/runtime": "^7.14.6",
    "esbuild": "^0.11.14",
    "typescript": "^5.0.0",
    "@types/uuid": "^8.3.0",
    "@types/three": "^0.166.0"
  },
  "type": "module"
}
